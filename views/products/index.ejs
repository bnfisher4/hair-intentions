<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <h1>Products</h1>
    <table>
        <thead>
            <tr>
                <th>Brand</th>
                <th>Kind</th>
            </tr>
        </thead>
        <tbody>
            <% products.forEach(function(p) { %>
            <tr>
                <td><%= p.brand %></td>
                <td><%= p.kind %></td>
            </tr>
            <% }); %>
        </tbody>
    </table>
</body>

</html>